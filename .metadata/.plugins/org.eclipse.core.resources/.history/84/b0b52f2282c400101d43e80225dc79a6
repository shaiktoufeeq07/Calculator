package Com.calci;

public class CalciImplementation implements Calci{

	@Override
	public int add(int a, int b) {
		int c=a+b;
		return c;
	}

	@Override
	public double sub(int a, int b) {
		int c=a-b;
		return c;
	}

	@Override
	public int mul(int a, int b) {
		 int c = a*b;
		 return c;
	}

	@Override
	public double div(int a, int b) {
		// TODO Auto-generated method stub
		return 0;
	}

}
